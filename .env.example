# 41Agent Configuration

# DashScope API Key (required for qwen3-omni-flash)
DASHSCOPE_API_KEY=your-api-key-here

# VM Configuration
VM_MEMORY=4G
VM_CPUS=4
VM_DISPLAY_RESOLUTION=1920x1080
VM_SCREENSHOT_FPS=30

# QEMU Paths
QEMU_SOCKET_PATH=/tmp/qemu-qmp.sock
QEMU_VNC_DISPLAY=:0

# Inochi2d Configuration
INOCHI2D_VMC_HOST=127.0.0.1
INOCHI2D_VMC_PORT=39540
INOCHI2D_AVATAR_PATH=assets/avatar.inx

# Memory Configuration
CHROMA_DB_PATH=db/chromadb
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Audio Configuration
AUDIO_SAMPLE_RATE=24000
AUDIO_VOICE=Cherry

# UI Configuration
UI_WIDTH=1920
UI_HEIGHT=1080
AVATAR_WIDTH=400
AVATAR_HEIGHT=600
